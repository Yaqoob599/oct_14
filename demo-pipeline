pipeline {
       agent { label 'java' }   
  
       stages {
              stage('checkout') {
                    steps {
                         git branch: 'main', url: 'https://github.com/saikiran-private/java-code-sample-with-unit-test-cases'
                    }
               }

               stage('test') {
                     steps {
                         echo 'running unit test and integration test'
                     }
               }

               stage('build') {
                     steps {
                         sh 'mvn clean package'
                     }
               }

               stage('push') {
                    steps {
                          echo 'pushing artifact to artifactory'
                     }
               }

       }

}
